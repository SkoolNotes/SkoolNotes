\hypertarget{organelles-in-eukaryotic-cells}{%
\section{Organelles in Eukaryotic
Cells}\label{organelles-in-eukaryotic-cells}}

\hypertarget{chloroplast-and-mitochondria}{%
\subsection{Chloroplast and
Mitochondria}\label{chloroplast-and-mitochondria}}

\begin{itemize}
\tightlist
\item
  \textbf{Chloroplast} --- found in plants + does photosynthesis
\item
  \textbf{Mitochondria} --- found in animals + store ATP and extract
  energy from ATP
\end{itemize}

\hypertarget{rough-endoplasmic-reticulum-er-and-smooth-er}{%
\subsection{Rough Endoplasmic Reticulum (ER) and Smooth
ER}\label{rough-endoplasmic-reticulum-er-and-smooth-er}}

\begin{itemize}
\tightlist
\item
  \textbf{Rough ER} --- covered by ribosomes and folds
  {[}{[}KBhBIO101Proteins{]}{]} proteins
\item
  \textbf{Smooth ER} --- not covered by ribosomes and makes
  {[}{[}KBhBIO101Lipids{]}{]} lipids
\end{itemize}

\hypertarget{ribosomes-and-golgi-apparatus}{%
\subsection{Ribosomes and Golgi
apparatus}\label{ribosomes-and-golgi-apparatus}}

\begin{itemize}
\tightlist
\item
  \textbf{Ribosomes} =\textgreater{} synthesizes proteins
\item
  \textbf{Golgi apparatus} =\textgreater{} packs, modifying, and moving
  proteins
\end{itemize}

\hypertarget{cell-wall-and-plasma-membrane}{%
\subsection{Cell Wall and Plasma
Membrane}\label{cell-wall-and-plasma-membrane}}

\begin{itemize}
\tightlist
\item
  \textbf{Cell Wall} --- found in plants =\textgreater{} surround the
  cell: hard
\item
  \textbf{Plasma membrane} --- found in animals =\textgreater{} surround
  the cell: soft {[}{[}KBhBIO101Lipids{]}{]} lipids
\end{itemize}

\hypertarget{cytosol-cytoplasm-and-cytoskeleton}{%
\subsection{Cytosol, Cytoplasm and
Cytoskeleton}\label{cytosol-cytoplasm-and-cytoskeleton}}

\begin{itemize}
\tightlist
\item
  \textbf{Cytosol} =\textgreater{} liquid found inside cells; the
  ``cytoplasm'' floats within it
\item
  \textbf{Cytoplasm} =\textgreater{} all the stuff within the cell
\end{itemize}

\hypertarget{nucleus-and-nucleolus}{%
\subsection{Nucleus and Nucleolus}\label{nucleus-and-nucleolus}}

\begin{itemize}
\tightlist
\item
  \textbf{Nucleus} =\textgreater{} centre of the cell, stores DNA
\item
  \textbf{Nucleolus} =\textgreater{} largest part of the nucleous that
  makes ribosomes
\end{itemize}

\hypertarget{lysosomes-and-food-vacuoles}{%
\subsection{Lysosomes and Food
Vacuoles}\label{lysosomes-and-food-vacuoles}}

\begin{itemize}
\tightlist
\item
  \textbf{Lysosomes} =\textgreater{} vesticles that contains enzymes
  that breaks down biomolecules
\item
  \textbf{Food Vacoules} =\textgreater{} vesticels that stores food and
  other resources
\end{itemize}

\hypertarget{cytoskeleton-and-microtubules}{%
\subsection{Cytoskeleton and
Microtubules}\label{cytoskeleton-and-microtubules}}

\begin{itemize}
\tightlist
\item
  \textbf{Cytoskeleton} =\textgreater{} complex network of proteins +
  fibres that organize the rest of the cell
\item
  \textbf{Microtubulues} =\textgreater{} Polymers of tubulin protein
  that provides the main structure of eukarotic cells
\end{itemize}

\hypertarget{flagella-and-cilia}{%
\subsection{Flagella and Cilia}\label{flagella-and-cilia}}

\begin{itemize}
\tightlist
\item
  \textbf{Flagella} =\textgreater{} a bacteria's tail --- allow them to
  move and also act as an sensory organ. longer than a cilla, and moves
  in sinosoidial pattern.
\item
  \textbf{Cilium} =\textgreater{} a cell's ``hair'' --- provides sensory
  and communications functions. Motil cilla could move about to ``grab''
  things, and non-motile cilla can't move. more abundant that the
  flagella, and moves in circular pattern if they do move, and moves in
  circular pattern if they do move
\end{itemize}

