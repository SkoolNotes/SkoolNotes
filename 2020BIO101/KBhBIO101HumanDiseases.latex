\centering
\includegraphics{Screen Shot 2020-10-12 at 11.22.35 PM.png}
\caption{Screen Shot 2020-10-12 at 11.22.35 PM.png}
\end{figure}

\textbf{Preventing Retroviruses}

\begin{itemize}
\tightlist
\item
  Prevent Fusion \texttt{gp120}, \texttt{gp41}, \texttt{CCR5}
\item
  Prevent reverse transcription \texttt{RT}
\item
  Prevent intergration via intergrease \texttt{IN}
\item
  Prevent viron maturation \texttt{PR}
\end{itemize}

\hypertarget{viral-genome-vs-mutation-rate}{%
\subsubsection{Viral Genome vs Mutation
Rate}\label{viral-genome-vs-mutation-rate}}

